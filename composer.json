{
    "name": "xsotechs/smart-payment-routing",
    "description": "A Laravel package that allows your application to intelligently select the best payment processor for each transaction.",
    "keywords": ["payment", "laravel", "composer", "package"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tope Olotu",
            "email": "topeolotu75@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "laravel/framework": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Xsotechs\\SmartPaymentRouting\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xsotechs\\SmartPaymentRouting\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xsotechs\\SmartPaymentRouting\\SmartPaymentRoutingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}